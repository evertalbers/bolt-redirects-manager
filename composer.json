{
    "name": "soapbox/redirectsmanager",
    "description": "A .htaccess based redirect manager for Bolt CMS.",
    "type": "bolt-extension",
    "keywords": [
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Hunt",
            "email": "robert.hunt@soapbox.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Soapbox\\RedirectsManager\\": "src",
            "Bolt\\Extension\\Soapbox\\RedirectsManager\\Exceptions\\": "exceptions"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Soapbox\\RedirectsManager\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Soapbox\\RedirectsManager\\RedirectsManagerExtension"
    }
}
